              <!--end::Svg Icon-->
            </span>
            </div>
          </div>
          <div class="navi-text">
            <div class="font-weight-bold">{{ 'My appointments'|t }}</div>
            <div class="text-muted">{{ 'View all your appointments'|t }}</div>
          </div>
        </div>
      </a>
      <!--end:Item-->
    </div>
    <!--end::Nav-->
    <!--begin::Separator-->
    <div class="separator separator-dashed my-7"></div>
    <!--end::Separator-->
    <!--begin::Notifications-->
    <div>
      <!--begin:Heading-->
      <h5 class="mb-5">{{ 'Upcoming appointments'|t }}</h5>
      <!--end:Heading-->
      {{ drupal_view('upcoming_appointments', 'block_1') }}
    </div>
    <!--end::Notifications-->
  </div>
  <!--end::Content-->
</div>
<!-- end::User Panel-->

