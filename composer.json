{
    "name": "verbruggenalex/project",
    "type": "project",
    "description": "Test project",
    "require": {
        "composer/installers": "^1.9",
        "custom/app_core": "@dev",
        "custom/bbc": "dev-master",
        "cweagans/composer-patches": "^1.6",
        "drupal/admin_toolbar": "^2.3",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/commerce_license": "^2.0@alpha",
        "drupal/config_override_warn": "^1.3",
        "drupal/config_split": "^1.7",
        "drupal/core": "^8.9",
        "drupal/core-composer-scaffold": "^8.9",
        "drupal/default_content": "^2.0@alpha",
        "drupal/duration_field": "^2.0",
        "drupal/fullcalendar_view": "^5.0",
        "drupal/geocoder": "^3.13",
        "drupal/geofield_map": "^2.67",
        "drupal/key": "^1.14",
        "drupal/mail_login": "^2.2",
        "drupal/office_hours": "^1.3",
        "drupal/pathauto": "^1.8",
        "drupal/profile": "^1.1",
        "drupal/realname": "^1.0@RC",
        "drupal/select_or_other": "^1.0@alpha",
        "drupal/smart_date": "^3.0",
        "drupal/smtp": "^1.0",
        "drupal/social_auth_facebook": "^2.0",
        "drupal/social_auth_google": "^2.1",
        "drupal/token": "^1.7",
        "drupal/toolbar_themes": "^1.0@alpha",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.8",
        "drupal/views_autocomplete_filters": "^1.3",
        "drupal/views_autosubmit": "^1.2",
        "drush/drush": "^10.3",
        "geocoder-php/google-maps-provider": "^4.5",
        "twbs/bootstrap": "^4.5"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "require-dev": {
        "custom/app_default_content": "dev-master",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.0",
        "drupal-composer/drupal-security-advisories": "dev-8.x-v2",
        "drupal/coder": "^8.3",
        "drupal/console": "^1.9",
        "drupal/core-dev": "^8.9",
        "drupal/devel": "^4.0",
        "drupal/devel_kint_extras": "^1.0",
        "drupal/drupal-extension": "^4.1",
        "ergebnis/composer-normalize": "^2.8"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "allowed-packages": [
                "drupal/core"
            ],
            "locations": {
                "web-root": "./web"
            },
            "file-mapping": {
                "[web-root]/sites/default/settings.php": "lib/drupal/web/sites/default/settings.php",
                "[web-root]/sites/development.services.yml": "lib/drupal/web/sites/development.services.yml",
                "[web-root]/libraries/bootstrap/dist/js/bootstrap.min.js": "vendor/twbs/bootstrap/dist/js/bootstrap.min.js"
            },
            "symlink": true,
            "overwrite": true
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/sites/{$name}": [
                "type:drupal-drupal-multisite"
            ],
            "vendor/drupal/console/{$name}": [
                "type:drupal-console"
            ],
            "vendor/drupal/console/language/{$name}": [
                "type:drupal-console-language"
            ],
            "config/{$name}/sync": [
                "type:drupal-config"
            ]
        },
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/core": [
                "https://www.drupal.org/files/issues/2020-05-04/2924549-38.patch",
                "https://www.drupal.org/files/issues/2019-12-09/drupal-language-prefix-neutral-content-2883450-31.patch"
            ],
            "drupal/commerce_license": [
                "https://www.drupal.org/files/issues/2020-01-14/3100029-5-license-overview-broken.patch"
            ],
            "drupal/fullcalendar_view": [
                "https://git.drupalcode.org/project/fullcalendar_view/-/merge_requests/1/diffs.patch"
            ],
            "drupal/smtp": [
                "https://www.drupal.org/files/issues/2020-08-11/removed_config_override_logic-2826189-39.patch"
            ],
            "drupal/token": [
                "https://www.drupal.org/files/issues/2020-09-21/token-consistent-entity-and-field-support-3047568-12.patch"
            ],
            "drupal/toolbar_themes": [
                "https://www.drupal.org/files/issues/2018-12-08/toolbar_themes-padding-issue-2981847-7.patch"
            ],
            "drupal/views_autocomplete_filters": [
                "https://www.drupal.org/files/issues/2020-08-14/autosubmit-on-select-1724278-10.patch"
            ]
        }
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "local": {
            "type": "path",
            "url": "lib/drupal/*/*"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "drupal-install-clean": [
            "mkdir -p web/sites/default/files/geofieldmap_icons",
            "ln -sf $(pwd)/lib/drupal/web/sites/default/files/translations $(pwd)/web/sites/default/files/translations",
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/drush si --existing-config --account-pass=admin --ansi"
        ],
        "build-bak": [
            "drush cex -y",
            "drush sql-dump --result-file=../dump.sql"
        ],
        "build-dist": [
            "composer install --no-dev --no-progress --no-suggest --ansi",
            "tar -zcf dist.tar.gz lib/drupal vendor/ web/ composer.json composer.lock"
        ],
        "deploy": [
            "mkdir -p build/dist/$2",
            "tar -zxf dist.tar.gz -C build/dist/$2"
        ],
        "set-permissions": [
            "[ $ENVIRONMENT != 'development'] && sudo chown -R docker:www-data",
            "find web -type d -exec chmod u=rwx,g=rx,o= '{}' \\;",
            "find web -type f -exec chmod u=rw,g=r,o= '{}' \\;",
            "find web/sites/default/files -type d -exec chmod ug=rwx,o= '{}' \\;",
            "find web/sites/default/files -type f -exec chmod ug=rw,o= '{}' \\;"
        ],
        "export-custom-translations": [
            "drush locale:export nl --types=customized > lib/drupal/web/sites/default/files/translations/custom-translations.nl.po",
            "drush locale:export fr --types=customized > lib/drupal/web/sites/default/files/translations/custom-translations.fr.po"
        ],
        "import-custom-translations": [
            "drush locale:import nl ../lib/drupal/web/sites/default/files/translations/custom-translations.nl.po --type=customized --override=all",
            "drush locale:import fr ../lib/drupal/web/sites/default/files/translations/custom-translations.fr.po --type=customized --override=all"
        ],
        "import-translations": [
            "drush locale-check",
            "drush locale-update"
        ],
        "test-behat": "./vendor/bin/behat",
        "test-phpcs": "./vendor/bin/phpcs"
    }
}
